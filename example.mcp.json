{
  "mcpServers": {
    "context7": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-context7",
        "npx",
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "browser-use": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-browser-use",
        "mcp-server-browser-use"
      ]
    },
    "postgres": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-postgres",
        "postgres-mcp",
        "--access-mode=unrestricted",
        "--transport=stdio",
        "postgresql://[USERNAME]:[PASSWORD]@[HOST]:[PORT]/[DATABASE]"
      ]
    },
    "sequential-thinking": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-sequentialthinking",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  }
}