{
  "mcpServers": {
    "context7": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-context7",
        "npx",
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "puppeteer": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-puppeteer",
        "npx",
        "-y",
        "puppeteer-mcp-server"
      ]
    },
    "zen": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-zen",
        "./run-server.sh"
      ]
    },
    "postgres": {
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-postgres",
        "postgres-mcp",
        "--access-mode=unrestricted",
        "--transport=stdio",
        "postgresql://postgres:postgres@host.docker.internal:5432/iris_rag"
      ]
    }
  }
}