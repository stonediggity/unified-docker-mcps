{
  "mcpServers": {
    "context7": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--name", "mcp-context7-temp",
        "mcp-context7:latest"
      ]
    },
    "puppeteer": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--name", "mcp-puppeteer-temp",
        "--cap-add", "SYS_ADMIN",
        "--security-opt", "seccomp=unconfined",
        "mcp-puppeteer:latest"
      ]
    },
    "zen": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--name", "mcp-zen-temp",
        "-e", "OPENROUTER_API_KEY=${OPENROUTER_API_KEY}",
        "mcp-zen:latest"
      ]
    },
    "postgres": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--name", "mcp-postgres-temp",
        "-e", "DATABASE_URL_STRING=${DATABASE_URL_STRING}",
        "mcp-postgres:latest"
      ]
    }
  }
}