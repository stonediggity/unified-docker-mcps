{
  "mcpServers": {
    "context7": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--name", "mcp-context7-temp",
        "mcp-context7:latest"
      ]
    },
    "browser-use": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--name", "mcp-browser-use-temp",
        "--cap-add", "SYS_ADMIN",
        "--security-opt", "seccomp=unconfined",
        "-e", "MCP_LLM_API_KEY=${OPENAI_API_KEY}",
        "-e", "MCP_BROWSER_HEADLESS=true",
        "mcp-browser-use:latest"
      ]
    },
    "postgres": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--name", "mcp-postgres-temp",
        "-e", "DATABASE_URL_STRING=${DATABASE_URL_STRING}",
        "mcp-postgres:latest"
      ]
    }
  }
}